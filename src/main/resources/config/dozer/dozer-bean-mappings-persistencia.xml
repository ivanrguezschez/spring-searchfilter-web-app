<?xml version="1.0" encoding="ISO-8859-1"?>
<mappings xmlns="http://dozer.sourceforge.net"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://dozer.sourceforge.net http://dozer.sourceforge.net/schema/beanmapping.xsd">

    <configuration>
        <trim-strings>true</trim-strings>
    </configuration>
	
    <mapping map-empty-string="false">
        <class-a>com.irs.springsearchfilterwebapp.dominio.Usuario</class-a>
        <class-b>com.irs.springsearchfilterwebapp.negocio.vo.UsuarioVO</class-b>
    </mapping>
	
    <mapping map-empty-string="false">
        <class-a>com.irs.springsearchfilterwebapp.dominio.Rol</class-a>
        <class-b>com.irs.springsearchfilterwebapp.negocio.vo.RolVO</class-b>
    </mapping>
    
    <mapping map-empty-string="false">
        <class-a>com.irs.springsearchfilterwebapp.dominio.TipoEntidad</class-a>
        <class-b>com.irs.springsearchfilterwebapp.negocio.vo.TipoEntidadVO</class-b>
    </mapping>
    
    <mapping map-empty-string="false">
        <class-a>com.irs.springsearchfilterwebapp.dominio.Filtro</class-a>
        <class-b>com.irs.springsearchfilterwebapp.negocio.vo.FiltroVO</class-b>
    </mapping>
</mappings>
